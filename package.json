{
  "name": "howtools",
  "version": "0.1.10",
  "description": "a simple tool",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npx cross-env NODE_ENV=development nodemon -e ts --exec ts-node ./src/index.ts",
    "build": "npx cross-env NODE_ENV=production tsc --declaration",
    "dev:docs": "npm run register:components --componentsDir=demo && vitepress dev docs",
    "build:docs": "npm run register:components && vitepress build docs",
    "serve:docs": "vitepress serve docs",
    "register:components": "vitepress-rc",
    "update:version": "ts-node ./build/updateVersion.ts"
  },
  "homepage": "https://github.com/biancangming/howtools",
  "repository": {
    "type": "git",
    "url": "git@github.com:biancangming/howtools.git"
  },
  "keywords": [],
  "author": "biancangming",
  "license": "ISC",
  "dependencies": {
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@types/node": "^16.9.1",
    "cross-env": "^6.0.3",
    "esbuild-register": "^3.0.0",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.4",
    "vite-svg-loader": "^2.2.0",
    "vitepress": "^0.16.1",
    "vitepress-theme-demoblock": "^1.2.6"
  }
}